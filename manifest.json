{
    "manifest_version": 3,
    "name": "Speed Controller",
    "version": "1.0",
    "description": "CS50's final project",
    "permissions": ["activeTab", "scripting", "storage"],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["https://*/*"],
        "js": [
            "content.js"
        ]
    }]
}